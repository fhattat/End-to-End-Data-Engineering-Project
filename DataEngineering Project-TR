splayName":"Instructor Data Scientist","userId":"05169733475914056098"}}},"execution_count":7,"outputs":[]},{"cell_type":"code","source":["# --- 2. ÃœRÃœN VERÄ°SÄ° (Products) ---\n","# Senaryo: 20 ÃœrÃ¼n. Kategori yazÄ±mlarÄ± tutarsÄ±z, varyantlar ismin iÃ§inde.\n","products = []\n","categories = ['Snack', 'supplement', 'Drink', 'SNACK', 'Supplement', 'drinK'] # BÃ¼yÃ¼k/kÃ¼Ã§Ã¼k harf karmaÅŸasÄ±\n","base_products = [\n","    ('Protein Bar', ['50g', '60g', '100g']),\n","    ('Whey Powder', ['1kg', '2kg', '500g']),\n","    ('Energy Drink', ['250ml', '500ml']),\n","    ('Oat Cookie', ['Pack', 'Single']),\n","    ('Multivitamin', ['30 tabs', '60 tabs'])\n","]\n","\n","product_id_counter = 1\n","for prod, variants in base_products:\n","    for var in variants:\n","        pid = f\"P{str(product_id_counter).zfill(3)}\"\n","\n","        # Hata Enjeksiyonu: %10 ihtimalle geÃ§ersiz ID\n","        if random.random() < 0.1:\n","            pid = \"INVALID_\" + str(random.randint(1,99))\n","\n","        p_name = f\"{prod} {var}\" # Varyant ismin iÃ§inde (Regex ile ayÄ±racaÄŸÄ±z)\n","        cat = random.choice(categories)\n","\n","        products.append([pid, p_name, cat])\n","        product_id_counter += 1\n","\n","df_products = pd.DataFrame(products, columns=['product_id', 'product_name', 'category'])\n"],"metadata":{"id":"eQ7Mc1Gfpyd_","executionInfo":{"status":"ok","timestamp":1765948388579,"user_tz":-180,"elapsed":58,"user":{"displayName":"Instructor Data Scientist","userId":"05169733475914056098"}}},"execution_count":8,"outputs":[]},{"cell_type":"code","source":["# --- 3. SÄ°PARÄ°Å VERÄ°SÄ° (Fact Orders) ---\n","# Senaryo: 1000 SipariÅŸ. Tarih formatlarÄ± karÄ±ÅŸÄ±k.\n","orders = []\n","start_date = datetime(2024, 1, 1)\n","\n","for i in range(1, 1001):\n","    oid = 1000 + i\n","    cid = random.choice(customer_ids)\n","\n","    # Hata Enjeksiyonu: Olmayan mÃ¼ÅŸteri ID'si (Foreign Key hatasÄ± simÃ¼lasyonu)\n","    if random.random() < 0.05:\n","        cid = 999\n","\n","    pid = random.choice(df_products['product_id'].tolist())\n","    qty = random.randint(1, 100)\n","    price = round(random.uniform(10, 500), 2)\n","\n","    # Tarih OluÅŸturma\n","    random_date = start_date + timedelta(days=random.randint(0, 365))\n","\n","    # Hata Enjeksiyonu: KarÄ±ÅŸÄ±k Tarih FormatlarÄ±\n","    rand_fmt = random.random()\n","    if rand_fmt < 0.6:\n","        date_str = random_date.strftime('%Y-%m-%d') # Standart: 2024-05-20\n","    elif rand_fmt < 0.9:\n","        date_str = random_date.strftime('%d-%m-%Y') # Ters: 20-05-2024\n","    else:\n","        date_str = random_date.strftime('%Y/%m/%d') # SlashlÄ±: 2024/05/20\n","\n","    orders.append([oid, cid, pid, date_str, qty, price])\n","\n","df_orders = pd.DataFrame(orders, columns=['order_id', 'customer_id', 'product_id', 'order_date', 'quantity', 'price'])"],"metadata":{"id":"XYVJDzyJpyhI","executionInfo":{"status":"ok","timestamp":1765948405016,"user_tz":-180,"elapsed":23,"user":{"displayName":"Instructor Data Scientist","userId":"05169733475914056098"}}},"execution_count":9,"outputs":[]},{"cell_type":"code","source":["# --- CSV KAYIT ---\n","df_customers.to_csv(os.path.join(folders['landing'], 'customers.csv'), index=False)\n","df_products.to_csv(os.path.join(folders['landing'], 'products.csv'), index=False)\n","df_orders.to_csv(os.path.join(folders['landing'], 'orders.csv'), index=False)\n","\n","print(f\"âœ… GeliÅŸmiÅŸ veri seti oluÅŸturuldu.\")\n","print(f\"ğŸ“Š MÃ¼ÅŸteri SayÄ±sÄ±: {len(df_customers)}\")\n","print(f\"ğŸ“Š ÃœrÃ¼n SayÄ±sÄ±: {len(df_products)}\")\n","print(f\"ğŸ“Š SipariÅŸ SayÄ±sÄ±: {len(df_orders)}\")\n","print(f\"ğŸ“‚ Konum: {folders['landing']}\")"],"metadata":{"colab":{"base_uri":"https://localhost:8080/"},"id":"6PYiBWavulV8","executionInfo":{"status":"ok","timestamp":1765948478016,"user_tz":-180,"elapsed":57,"user":{"displayName":"Instructor Data Scientist","userId":"05169733475914056098"}},"outputId":"338bcd9a-1216-4ead-bcc2-17f1976bfdee"},"execution_count":11